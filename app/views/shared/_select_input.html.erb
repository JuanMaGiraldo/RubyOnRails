<div class ="form-group row">
  <%= form.label key, {class:"col-sm-2 col-form-label"} %>
  <div class="col-sm-10">
    
    <%= form.select key, select_list, {}, { class: 'form-control'} %>

  </div> 
</div>

<% obj.errors.full_messages_for(key).each do |message| %>
  <span class="help-block mt-3"><%= message%> </span>
<%end%>


